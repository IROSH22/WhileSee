<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".sustainable_practices_main"
    android:background="@drawable/sust_main">

    <TextView
        android:id="@+id/textView33"
        android:layout_width="320dp"
        android:layout_height="58dp"
        android:fontFamily="serif"
        android:gravity="center"
        android:text="Sustainable Practices"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.075" />

    <Button
        android:id="@+id/button39"
        style="@style/Widget.AppCompat.Button.Borderless.Colored"
        android:layout_width="330dp"
        android:layout_height="54dp"
        android:backgroundTint="#00FFFFFF"
        android:backgroundTintMode="multiply"
        android:drawableTint="#96FFFFFF"
        android:fontFamily="serif"
        android:gravity="center"
        android:text="Sustainable Fishing"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.317" />

    <Button
        android:id="@+id/button40"
        style="@style/Widget.AppCompat.Button.Borderless.Colored"
        android:layout_width="330dp"
        android:layout_height="54dp"
        android:backgroundTint="#00FFFFFF"
        android:fontFamily="serif"
        android:text=" Reduce Plastic Waste"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499" />

    <Button
        android:id="@+id/button41"
        style="@style/Widget.AppCompat.Button.Borderless.Colored"
        android:layout_width="330dp"
        android:layout_height="54dp"
        android:backgroundTint="#00FFFFFF"
        android:text="Marine Protected Areas (MPAs)"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.704" />

    <Button
        android:id="@+id/button44"
        android:layout_width="111dp"
        android:layout_height="57dp"
        android:fontFamily="serif"
        android:text="Back"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.928" />
</androidx.constraintlayout.widget.ConstraintLayout>